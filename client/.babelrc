{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins":[
    ["transform-runtime",{
        "helpers" : false,
        "polyfill" : false,
        "regenerator" : true,
        "moduleName" : "babel-runtime"
    }],
    "syntax-dynamic-import",
    ["component",{
        "libraryName" : "element-ui",
        "styleLibraryName" : "theme-chalk" 
    }]
]
}
